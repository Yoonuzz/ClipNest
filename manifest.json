{
  "manifest_version": 3,
  "name": "Web Snippet Saver (ClipNest)",
  "version": "1.1",
  "description": "Copy text and images from web pages and save them into a stylish notepad-style snippet keeper with animations and sync.",
  "author": "© 2025 ClipNest (Mohammed Yoonus M)",
  "copyright": "© 2025 ClipNest. All rights reserved.",
  
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
 "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
